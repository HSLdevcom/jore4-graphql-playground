{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^13.1.4",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "babel-plugin-relay": "^10.1.3",
    "graphql": "^15.5.0",
    "http-proxy-middleware": "^1.0.6",
    "leaflet": "^1.7.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-leaflet": "^3.0.5",
    "react-relay": "^11.0.0",
    "react-scripts": "4.0.1",
    "subscriptions-transport-ws": "^0.9.18",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "yarn prebuild && REACT_APP_BACKEND_HOST=localhost:8080 react-scripts start",
    "prebuild": "yarn relay",
    "build": "yarn prebuild && react-scripts build",
    "test": "yarn prebuild && react-scripts test",
    "test:ci": "yarn test --ci",
    "lint": "eslint --max-warnings=0",
    "lint:all": "yarn lint './src/**/*.{ts,tsx}'",
    "eject": "react-scripts eject",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "relay": "relay-compiler",
    "schema:update": "sh scripts/import-graphql-schema.sh",
    "ts:check": "yarn prebuild && yarn tsc --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/leaflet": "^1.5.21",
    "@types/react-relay": "^7.0.17",
    "@types/relay-runtime": "^10.1.8",
    "graphqurl": "^0.3.3",
    "prettier": "^2.2.1",
    "relay-compiler": "^11.0.0",
    "relay-compiler-language-typescript": "^13.0.6",
    "relay-config": "^10.1.3"
  }
}
